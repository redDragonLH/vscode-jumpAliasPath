{
	"name": "jump-alias-path",
	"displayName": "jumpAliasPath",
	"description": "jumpAliasPath",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.40.0"
	},
	"publisher": "liuhe945",
	"keywords": [
		"alias",
		"webpack",
		"别名跳转"
	],
	"categories": [
		"Other"
	],
	"icon": "favicon.ico",
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
		"type": "object",
		"title": "fileAliasjumpSetting",
		"properties": {
				"fileAliasSetting.aliasMap": {
						"type": "object",
						"default": {},
						"scope": "resource",
						"description": "alias 映射表，同webpack配置"
				},
				"fileAliasSetting.configPath": {
						"type": "object",
						"default": {
							"default":"webpack.config.js"
						},
						"scope": "resource",
						"description": "webpack config 路径,可以以项目文件夹分别设置"
				},
				"fileAliasSetting.fileSuffixs": {
					 "type": "array",
						"scope": "resource",
						"default": [],
					 "description": "需要识别语言的后缀"
				}
		 }
	 }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:redDragonLH/vscode-jumpAliasPath.git"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
